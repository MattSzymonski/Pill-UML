@start_style
// Override: use cyan arrows instead of default
.message { stroke: #00ccff; }
.arrow-head { fill: #00ccff; }
.arrow-head-open { stroke: #00ccff; }
@end_style

@start_uml
participant Client
participant Server
participant Database

Client -> Server: HTTP Request
Server -> Database: Query
Database --> Server: Result
Server --> Client: HTTP Response

alt Success
    Client -> Client: Display Data
else Failure
    Client -> Client: Show Error
end
@end_uml
